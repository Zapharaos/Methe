{
  "expo": {
    "name": "Methe",
    "slug": "Methe",
    "scheme": "methe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/icon-dark.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#FEECCA",
        "image": "./assets/splash/splash-light.png",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#4D3E3E",
          "image": "./assets/splash/splash-dark.png",
          "resizeMode": "contain"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive-icon-dark.png",
        "backgroundColor": "#4D3E3E"
      },
      "splash": {
        "backgroundColor": "#FEECCA",
        "image": "./assets/splash/splash-light.png",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#4D3E3E",
          "image": "./assets/splash/splash-dark.png",
          "resizeMode": "contain"
        }
      },
      "package": "com.zapharaos.Methe"
    },
    "web": {
      "favicon": "./assets/icon/favicon.png"
    },
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": ""
      }
    },
    "plugins": [
      "expo-localization",
      "expo-updates",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
